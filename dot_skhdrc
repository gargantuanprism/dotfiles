shift + cmd - b : yabai -m space --balance
shift + cmd - r : yabai -m space --rotate 90
shift + cmd - t : yabai -m space --layout stack
shift + cmd - p : yabai -m space --layout bsp
shift + cmd - e : yabai -m space --equalize
shift + cmd - y : yabai -m space --mirror y-axis
shift + cmd - x : yabai -m space --mirror x-axis
shift + cmd - space : yabai -m window --swap first
shift + cmd - l : yabai -m window --swap next
shift + cmd - h : yabai -m window --swap prev
shift + cmd - s : yabai -m window --toggle split
shift + cmd - f : yabai -m window --toggle float
shift + cmd - z : yabai -m window --toggle zoom-fullscreen
shift + cmd - m : yabai -m window --minimize

shift + cmd - 1 : yabai -m window --space 1
shift + cmd - 2 : yabai -m window --space 2
shift + cmd - 3 : yabai -m window --space 3
shift + cmd - 4 : yabai -m window --space 4
shift + cmd - 5 : yabai -m window --space 5
shift + cmd - 6 : yabai -m window --space 6

# cmd (win) + tilde - cycle windows
cmd - 0x32 : ~/bin/yabai-cycle.sh

shift + cmd + alt - r : yabai --restart-service

